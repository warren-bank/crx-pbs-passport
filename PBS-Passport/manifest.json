{
  "name": "PBS Passport",
  "version": "0.1.1",
  "description": "Enables videos on PBS website to play without a PBS Passport.",
  "content_scripts": [{
    "matches": ["*://player.pbs.org/*"],
    "all_frames": true,
    "run_at": "document_start",
    "js": ["content.js"]
  }],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2
}
